<schema>
	<element tablename="waterworks_newconnection">
		<field name="objid" primary="true" prefix="WNCONN" />
		<field name="controlno" required="true" />
		<field name="dtcreated" required="true" />

		<complex name="account" ref="vw_waterworks_account_info" jointype="many-to-one" required="true">
			<key field="acctinfoid" target="objid" />
		</complex>	

		<complex name="task" ref="waterworks_newconnection_task" jointype="many-to-one" required="false">
			<key field="taskid" target="taskid" />
		</complex>	

	</element>
</schema>